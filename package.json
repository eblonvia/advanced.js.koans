{
  "name": "javascript.koans",
  "version": "1.0.0",
  "description": "In this set of examples and tests we cover advanced JavaScript concepts such as closure, inheritance, variable hoisting, 'this' with context switch...",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-loader": "^5.4.2",
    "babel-runtime": "^5.8.25",
    "chai": "^3.3.0",
    "esdoc": "^0.4.3",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "live-server": "^0.9.1",
    "mocha": "^2.3.3",
    "node-inspector": "^0.12.5",
    "npm-run-all": "^3.1.1",
    "sinon": "^1.17.2",
    "wallaby-webpack": "0.0.9",
    "webpack": "^1.13.2",
    "yarn": "^0.16.1"
  },
  "scripts": {
    "test": "mocha --compilers js:babel/register -w",
